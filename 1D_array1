import java.util.Scanner;
class Array {
    public static void main(String args[]){
//        int a[] = {3,4,5,6,7};
        int a[] = new int[5];
        Scanner in = new Scanner(System.in);
        System.out.println("Enter the elements");
        for(int i=0;i<5;i++){
           a[i] = in.nextInt();
        }
        System.out.println("Elements are ");
        for(int i=0;i<5;i++){
            System.out.println(a[i]);
        }
    }
}
